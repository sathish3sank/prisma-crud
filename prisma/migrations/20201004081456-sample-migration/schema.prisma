generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = "***"
}

model user {
  id               Int            @id @default(autoincrement())
  first_name       String
  last_name        String
}

model contactdetails {
  id     Int     @id @default(autoincrement())
  mobile String  @unique
  home   String?
  father String
  mother String?
  user   user    @relation(fields: [userId], references: [id])
  userId Int
}
