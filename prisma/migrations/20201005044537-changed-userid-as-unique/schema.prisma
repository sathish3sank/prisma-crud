generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = "***"
}

model users {
  id             Int              @id @default(autoincrement())
  first_name     String
  last_name      String
  contactdetails contactdetails[]
}

model contactdetails {
  id     Int     @id @default(autoincrement())
  mobile String  @unique
  home   String?
  father String
  mother String?
  user   users   @relation(fields: [userId], references: [id])
  userId Int     @unique
}
